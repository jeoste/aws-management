{
  "version": 2,
  "rewrites": [
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/api/(.*)", "destination": "/api/index" },
    { "source": "/(.*)", "destination": "/api/index" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.1.0"
    }
  }
}
